  "homepage": "https://github.com/kcjpop/coverage-comments#readme",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "lcov-parse": "1.0.0",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@vercel/ncc": "0.36.1",
    "eslint": "8.39.0",
    "jest": "29.5.0"
